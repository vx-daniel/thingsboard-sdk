{
  "name": "thingsboard-sdk",
  "version": "1.0.0",
  "description": "A strongly-typed TypeScript SDK for interacting with the ThingsBoard REST API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "go": "npm run generate && npm run build && npm run docs",
    "build": "tsc",
    "generate": "ts-node scripts/generate.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "typedoc src/index.ts src/base-client.ts src/generated/index.ts --out docs"
  },
  "keywords": [
    "thingsboard",
    "sdk",
    "typescript",
    "iot",
    "api",
    "rest",
    "client",
    "fetch",
    "zod",
    "validation"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vx-daniel/thingsboard-sdk"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^6.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.3",
    "typescript": "^5.3.3"
  }
}
